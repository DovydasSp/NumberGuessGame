<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dovspe.test2"
    android:targetSandboxVersion="2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="28" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@mipmap/logo_main_corners"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo_main_circle"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.dovspe.NumberGuessGame.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.dovspe.NumberGuessGame.ActivityAbout" />
        <activity android:name="com.example.dovspe.NumberGuessGame.SettingActivity" />
        <activity android:name="com.example.dovspe.NumberGuessGame.LevelSelectActivity" />
        <activity android:name="com.example.dovspe.NumberGuessGame.PlayActivity" />
        <activity android:name="com.example.dovspe.NumberGuessGame.WinLoseActivity" />
        <activity android:name="com.example.dovspe.NumberGuessGame.StatsActivity" />

        <provider
            android:name="com.android.tools.ir.server.InstantRunContentProvider"
            android:authorities="com.example.dovspe.test2.com.android.tools.ir.server.InstantRunContentProvider"
            android:multiprocess="true" />
    </application>

</manifest>